    <string name="notifications_enabled_title">後台通知</string>
    <string name="notifications_enabled_summary">開啟後，你將可以收到Facebook的後台通知。</string>

    <string name="notification_interval_title">後台通知時間間隔</string>
    <string name="notification_interval_summary">設定「後台通知」的更新頻率。</string>
